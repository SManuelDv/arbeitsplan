{
  "name": "arbeittsplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "npm run kill-port",
    "dev": "cross-env NODE_ENV=development FORCE_COLOR=1 vite",
    "dev:host": "npm run predev && cross-env NODE_ENV=development FORCE_COLOR=1 vite --host",
    "dev:force": "npm run predev && cross-env NODE_ENV=development FORCE_COLOR=1 vite --force",
    "start": "npm run dev:host",
    "build": "CI=false tsc --noEmit false && vite build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "preview": "cross-env NODE_ENV=production vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "vercel-build": "CI=false tsc --noEmit false && vite build",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "clean": "rimraf node_modules/.vite dist",
    "dev:clean": "npm run kill-port && npm run clean && npm run dev",
    "dev:reset": "npm run kill-port && npm run clean && npm run dev",
    "kill-port": "kill-port 3000 || true",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck",
    "lint:prod": "eslint src/",
    "validate:prod": "npm run typecheck && npm run lint:prod",
    "build:prod": "npm run validate:prod && npm run build",
    "analyze:bundle": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/react-query": "^5.65.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.1",
    "i18next": "24.2.2",
    "i18next-browser-languagedetector": "8.0.2",
    "i18next-http-backend": "3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.1",
    "react-i18next": "15.4.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^6.28.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime-corejs3": "^7.24.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.10",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.17",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "^5.4.1",
    "core-js": "3.40.0",
    "cross-env": "7.0.3",
    "cssnano": "6.1.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^14.0.0",
    "husky": "^9.1.7",
    "kill-port": "2.0.1",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.33",
    "postcss-preset-env": "9.6.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.1",
    "terser": "5.39.0",
    "tsx": "^4.19.2",
    "typescript": "5.3.3",
    "vite": "^5.0.12"
  }
}
